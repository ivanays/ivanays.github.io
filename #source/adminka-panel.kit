          <div class="popup-direct__header">
            <h2 class="popup-direct__title">Создать услугу</h2>
          </div>
          <div class="popup-direct__content">
            <div class="popup-direct__form">
              <form action="#" class="form" name="form-create-b" id="form-create-b" enctype="multipart/form-data">
                <div class="form__inner">
                  <div class="form__left">
                    <div class="form__block">
                      <label for="" class="form__label">Слайд</label>
                      <div class="form__phone-wrapper form__phone-trim3">
                        <span id="form-create-b-slide-error" class="form__error"></span>
                        <div class="form__phone-wrapper form__phone-trim4">
                          <input type="file" accept="image/*, .pdf" min="0" name="form-create-b-slide"
                            id="form-create-b-slide" class="form__input form__input-file">
