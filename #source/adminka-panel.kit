      <div id="popup-create-a" class="popup-direct">
        <div class="popup-direct__body">
          <div class="popup-direct__header">
            <h2 class="popup-direct__title">Создать окно</h2>
          </div>
          <div class="popup-direct__content">
            <div class="popup-direct__form">
              <form action="#" method="post" class="form" name="form-create-a" id="form-create-a">
                <div class="form__inner-culomn">
                  <div class="form__left form__left-a">
                    <div class="form__block">
                      <label for="form-create-a-startdate" class="form__label">Начало /
                        Дата</label>
                      <div class="form__phone-wrapper">
                        <span id="form-create-a-startdate-error" class="form__error"></span>
                        <input type="date" name="form-create-a-startdate" id="form-create-a-startdate"
                          class="form__input" required>
                      </div>
                    </div>
                    <div class="form__block">
                      <label for="form-create-a-starttime" class="form__label">Начало /
                        Время</label>
                      <div class="form__phone-wrapper">
                        <span id="form-create-a-starttime-error" class="form__error"></span>
                        <input type="time" name="form-create-a-starttime" id="form-create-a-starttime"
                          class="form__input" required>
                      </div>
                    </div>
                  </div>
                  <div class="form__right form__right-a">
                    <div class="form__block">
                      <label for="form-create-a-enddate" class="form__label">Конец /
                        Дата</label>
                      <div class="form__phone-wrapper">
                        <span id="form-create-a-enddate-error" class="form__error"></span>
                        <input type="date" name="form-create-a-enddate" id="form-create-a-enddate" class="form__input"
                          required>
                      </div>
                    </div>
                    <div class="form__block">
                      <label for="form-create-a-endtime" class="form__label">Конец /
                        Время</label>
                      <div class="form__phone-wrapper">
                        <span id="form-create-a-endtime-error" class="form__error"></span>
                        <input type="time" name="form-create-a-endtime" id="form-create-a-endtime" class="form__input"
                          required>
                      </div>
                    </div>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
